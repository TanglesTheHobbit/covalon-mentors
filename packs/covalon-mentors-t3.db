,"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["fortune"],"rarity":"common","custom":""},"rules":[],"slug":"true-strike","schema":{"version":0.764,"lastMigration":{"datetime":"2022-07-01T09:58:27.732-04:00","version":{"schema":0.763,"foundry":"9.269","system":"3.12.2.10"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":false,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"1"},"duration":{"value":"until the end of your turn"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"JOj2Gv9NFGrmb9Bl"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aNDEqRNv3n6sqn54":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Gb7SeieEvd0pL2Eh"}}},{"_id":"Hu38hoAUSYeFpkVa","name":"Force Bolt","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/force-bolt.webp","data":{"description":{"value":"<p>You fire an unerring dart of force from your fingertips. It automatically hits and deals 1d4+1 force damage to the target.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The damage increases by 1d4+1.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["force","wizard"],"rarity":"uncommon","custom":""},"rules":[],"slug":"force-bolt","schema":{"version":0.764,"lastMigration":{"datetime":"2022-07-01T09:58:27.732-04:00","version":{"schema":0.763,"foundry":"9.269","system":"3.12.2.10"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"focus"},"traditions":{"val